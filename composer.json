{
    "name": "cmrignite/drupal-composer-managed",
    "description": "Install Drupal 10 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        }
    ],
    "require": {
        "pantheon-upstreams/upstream-configuration": "dev-main",
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-recommended": "^10",
        "pantheon-systems/drupal-integrations": "^10",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^11 || ^12",
        "drupal/linkit": "^6.1.2",
        "drupal/components": "^3.0@beta",
        "drupal/twig_tweak": "^3.2.1",
        "drupal/pathauto": "^1.12",
        "drupal/token": "^1.13",
        "drupal/paragraphs": "1.17",
        "drupal/admin_toolbar": "^3.4.2",
        "drupal/layout_paragraphs": "^2.0.4",
        "drupal/webform": "^6.2.2@beta",
        "drupal/block_field": "^1.0@RC",
        "drupal/paragraphs_limits": "^1.0@alpha",
        "drupal/linked_field": "^1.5",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/convert_bundles": "^2.0@beta",
        "drupal/editor_button_link": "^2.0",
        "drupal/extlink": "^1.7",
        "drupal/maxlength": "^2.1.2",
        "drupal/ratio_crop": "^2.0",
        "drupal/simple_sitemap": "^4.1.8",
        "drupal/paragraphs_collapsible": "^1.4",
        "drupal/imageapi_optimize": "^4.0.1",
        "drupal/imageapi_optimize_resmushit": "^2.0@beta",
        "drupal/image_widget_crop": "^2.4",
        "drupal/field_group": "^3.4",
        "drupal/metatag": "^2.0",
        "drupal/simplify_menu": "^3.1.1",
        "drupal/views_field_formatter": "^4.0.1",
        "drupal/field_states_ui": "^3.0",
        "drupal/twig_field_value": "^2.0.2",
        "drupal/menu_item_extras": "^3.0.2",
        "drupal/menu_link_attributes": "^1.3",
        "drupal/yaml_editor": "^1.2",
        "drupal/redirect": "^1.9",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/datetime_range_timezone": "^1.0@alpha",
        "drupal/string_translation_ui": "^2.0.1",
        "drupal/filefield_paths": "^1.0@beta",
        "drupal/media_directories": "^2.1@beta",
        "drupal/entity_print": "^2.13",
        "drupal/smtp": "^1.2",
        "drupal/media_library_edit": "^3.0.3",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/media_bulk_upload": "^3.0.2",
        "drupal/feeds": "^3.0@beta",
        "drupal/tagify": "^1.2.4",
        "league/csv": "^9.8",
        "smalot/pdfparser": "^2.8",
        "drupal/diff": "^1.1",
        "drupal/verf": "^2.0.2",
        "drupal/lupus_taxonomy_import": "^1.2",
        "drupal/dropzonejs": "^2.8",
        "enyo/dropzone": "^5.7",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/upgrade_status": "^4.0",
        "drupal/search_api": "^1.34",
        "drupal/search_api_solr": "4.x-dev@dev",
        "drupal/file_mdm": "^3.0",
        "drupal/views_bulk_edit": "^2.9",
        "drupal/views_bulk_operations": "^4.2.6",
        "drupal/ckeditor_link_styles": "^1.0.1",
        "wikimedia/composer-merge-plugin": "^2.1",
        "mglaman/composer-drupal-lenient": "^1.0.4",
        "drupal/save_edit": "^1.7",
        "drupal/entity_embed": "^1.6",
        "drupal/entity_browser": "^2.10",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/multiple_fields_remove_button": "^2.2",
        "drupal/views_data_export": "^1.4",
        "drupal/migrate_plus": "^6.0.2",
        "drupal/ckeditor_liststyle": "^1.5",
        "drupal/bpmn_io": "^1.1.3",
        "drupal/field_validation": "^1.1",
        "drupal/typed_data": "^2.0",
        "drupal/hotkeys_for_save": "^2.0.1",
        "drupal/media_entity_file_replace": "^1.2",
        "drupal/core-project-message": "^10",
        "drupal/ckeditor5_dev": "^1.0.3",
        "ext-dom": "*",
        "drupal/layout_paragraphs_limit": "^1.0@RC",
        "drupal/backup_migrate": "^5.0.3",
        "drupal/ckeditor5_embedded_content": "^1.0.1",
        "drupal/editor_advanced_link": "^2.2.4",
        "drupal/color_field": "^3.0",
        "drupal/paragraphs_asymmetric_translation_widgets": "^1.3",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/config_split": "^1.9",
        "drupal/simplei": "^2.1.4",
        "drupal/svg_image": "^3.0.2",
        "drupal/easy_breadcrumb": "^2.0.6",
        "drupal/media_duplicates": "^2.0.2",
        "drupal/scheduler": "^2.0.1",
        "drupal/scheduler_content_moderation_integration": "^3.0",
        "drupal/views_ajax_history": "^1.7",
        "drupal/config_ignore": "^3.2",
        "drupal/token_or": "^2.2",
        "drupal/token_filter": "^2.1",
        "drupal/token_custom": "^1.0@alpha",
        "drupal/file_rename": "^1.0.1",
        "drupal/anchor_link": "^3.0@alpha",
        "drupal/autocomplete_deluxe": "^2.0",
        "drupal/media_entity_download": "^2.2",
        "drupal/asset_injector": "^2.19",
        "drupal/bigmenu": "^2.0@RC",
        "drupal/file_delete": "^2.0",
        "drupal/exclude_node_title": "^1.4",
        "drupal/taxonomy_access_fix": "^4.0",
        "drupal/single_content_sync": "^1.4"
    },
    "require-dev": {
        "drupal/core-dev": "^10",
        "drupal/twig_xdebug": "^1.3",
        "drupal/devel_php": "^1.6",
        "drupal/coder": "^8.3.22",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "squizlabs/php_codesniffer": "^3.8.1",
        "drupal/devel": "^5.1.2",
        "drupal/devel_kint_extras": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": ["pantheon-systems/drupal-integrations"],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "enable-patching": true
    },
    "autoload": {
        "classmap": ["upstream-configuration/scripts/ComposerScripts.php"]
    },
    "scripts": {
        "pre-update-cmd": ["DrupalComposerManaged\\ComposerScripts::preUpdate"],
        "post-update-cmd": ["DrupalComposerManaged\\ComposerScripts::postUpdate"]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "8.2.0"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "tbachert/spi": true,
            "drupal/core-project-message": true,
            "mglaman/composer-drupal-lenient": true,
            "wikimedia/composer-merge-plugin": true
        }
    }
}
